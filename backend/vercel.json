{
    "version": 2,
    "name": "talewind-fastapi",
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "entrypoint": "app:app",
          "requirementsTxtPath": "requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/predict/(.*)",
        "dest": "/predict"
      }
    ],
    "regions": ["sin1"]
  }